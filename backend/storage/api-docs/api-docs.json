        "title": "DeviceTrackingStatus",
        "version": "0.1"
    },
    "paths": {
        "/api/audits": {
            "get": {
                "tags": [
                    "Audit"
                ],
                "summary": "Retrieve a list of audit entries",
                "operationId": "db0a1e94b24353174d79a38019a05c3d",
                "responses": {
                    "200": {
                        "description": "List of audits",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Audit"
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No audits found"
                    }
                }
            }
        },
        "/api/audits/{id}": {
            "get": {
                "tags": [
                    "Audit"
                ],
                "summary": "Retrieve a single audit entry",
                "operationId": "daa15b2babe779fa0f9f6c66a4936490",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Audit ID",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Audit retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Audit"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Audit not found"
                    }
                }
            }
        }
    },
